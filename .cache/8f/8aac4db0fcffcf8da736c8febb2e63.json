","original":{"line":131,"column":20},"generated":{"line":1,"column":2546}},{"source":"main.js","name":"errorID","original":{"line":131,"column":23},"generated":{"line":1,"column":2548}},{"source":"main.js","original":{"line":131,"column":32},"generated":{"line":1,"column":2557}},{"source":"main.js","name":"error","original":{"line":132,"column":4},"generated":{"line":1,"column":2560}},{"source":"main.js","name":"textContent","original":{"line":132,"column":10},"generated":{"line":1,"column":2562}},{"source":"main.js","name":"errorMessage","original":{"line":132,"column":24},"generated":{"line":1,"column":2574}},{"source":"main.js","name":"setTimeout","original":{"line":134,"column":4},"generated":{"line":1,"column":2576}},{"source":"main.js","original":{"line":134,"column":15},"generated":{"line":1,"column":2587}},{"source":"main.js","name":"error","original":{"line":135,"column":6},"generated":{"line":1,"column":2598}},{"source":"main.js","name":"textContent","original":{"line":135,"column":12},"generated":{"line":1,"column":2600}},{"source":"main.js","original":{"line":135,"column":26},"generated":{"line":1,"column":2612}},{"source":"main.js","name":"time","original":{"line":136,"column":7},"generated":{"line":1,"column":2618}},{"source":"main.js","name":"$","original":{"line":139,"column":2},"generated":{"line":1,"column":2623}},{"source":"main.js","name":"document","original":{"line":139,"column":4},"generated":{"line":1,"column":2625}},{"source":"main.js","name":"ready","original":{"line":139,"column":14},"generated":{"line":1,"column":2635}},{"source":"main.js","original":{"line":139,"column":20},"generated":{"line":1,"column":2641}},{"source":"main.js","name":"me","original":{"line":140,"column":4},"generated":{"line":1,"column":2652}},{"source":"main.js","name":"setListeners","original":{"line":140,"column":7},"generated":{"line":1,"column":2654}},{"source":"main.js","original":{"line":4,"column":0},"generated":{"line":1,"column":2671}},{"source":"main.js","name":"jQuery","original":{"line":143,"column":3},"generated":{"line":1,"column":2672}},{"source":"main.js","name":"noConflict","original":{"line":143,"column":10},"generated":{"line":1,"column":2674}}],"sources":{"main.js":"import * as jQuery from 'jquery';\r\nimport { fabrics, cities, clothingTypes, data } from './data';\r\n\r\n(function ($) {\r\n\r\n  let me = {\r\n    vrstaTkanineID: \"#vrstaTkanine\",\r\n    sadrzajTkanineID: \"#sadrzajTkanine\",\r\n    vrstaOdeceID: \"#vrstaOdece\",\r\n    lokacijaID: \"#lokacija\",\r\n    proizvodiID: \"#product-list\",\r\n    submitButtonID: \"#submit\",\r\n    formID: \"#laundry-form\",\r\n    errorID: \"#error\"\r\n  }\r\n\r\n  me.templateFactory = (url, name, distributor, price) => {\r\n    return `<div class=\"product p-2 bg-white rounded d-flex flex-column flex-wrap align-items-center\">\r\n        <div class=\"image-wrapper d-flex justify-content-center pb-2\">\r\n          <img src=\"${url}\"\r\n                alt=\"${name}\">\r\n        </div>\r\n        <p>${name}</p>\r\n        <p>${distributor}</p>\r\n        <p class=\"h4\">Cena: <span>${price}</span></p>\r\n      </div>`\r\n  }\r\n\r\n  me.renderProducts = (data) => {\r\n    data.forEach(\r\n      item => {\r\n        // console.log('rendering product');\r\n\r\n        $(me.proizvodiID).append(me.templateFactory(\r\n          item.imageURL,\r\n          item.name,\r\n          item.distributor,\r\n          item.price\r\n        ))\r\n      }\r\n    )\r\n  }\r\n\r\n  me.setListeners = () => {\r\n    $(me.submitButtonID).on('click', (event) => {\r\n      const form = $(me.formID);\r\n      form.submit(me.handleSubmit(form));\r\n    });\r\n  }\r\n\r\n  me.handleSubmit = (form) => {\r\n    // console.log('form submitted');\r\n    let isError = false;\r\n    let resultList = data;\r\n    // clean the data so far\r\n    $(me.proizvodiID).empty();\r\n\r\n    const vrstaTkanine = form.find(me.vrstaTkanineID).val().toLowerCase();\r\n\r\n    if (fabrics.includes(vrstaTkanine)) {\r\n      // console.log(`there is vrstaTkanine: ${vrstaTkanine}`);\r\n      resultList = resultList.filter(item => item.fabric === vrstaTkanine);\r\n    } else {\r\n      isError = true;\r\n      // console.log('error with vrstaTkanine');\r\n      me.showTimedError(\"Ne postoji takva vrsta tkanine u našoj bazi, pokušajte sa nekom drugom.\");\r\n      return false;\r\n    }\r\n\r\n    const sadržajTkanine = +form.find(me.sadrzajTkanineID).val().toLowerCase();\r\n\r\n    if (!isError) {\r\n      // console.log(`there is sadržajTkanine: ${sadržajTkanine}`);\r\n      resultList = resultList.filter(item => item.fabricPercent <= sadržajTkanine);\r\n    } else {\r\n      isError = true;\r\n      // console.log('error with sadržajTkanine');\r\n      return false;\r\n    }\r\n\r\n    const vrstaOdece = form.find(me.vrstaOdeceID).val().toLowerCase();\r\n\r\n    if (clothingTypes.includes(vrstaOdece) && !isError) {\r\n      // console.log(`there is vrstaOdece: ${vrstaOdece}`);\r\n      resultList = resultList.filter(item => item.clothingType === vrstaOdece);\r\n    } else if (vrstaOdece === '') {\r\n\r\n    } else {\r\n      isError = true;\r\n      // console.log('error with vrstaOdece');\r\n      me.showTimedError(\"Ne postoji takva vrsta odeće u našoj bazi, pokušajte sa nekom drugom.\");\r\n      return false;\r\n    }\r\n\r\n    let lokacija = form.find(me.lokacijaID).val().toLowerCase();\r\n\r\n    if (cities.includes(lokacija) && !isError) {\r\n      // console.log(`there is lokacija: ${lokacija}`);\r\n      resultList = resultList.filter(item => item.city === lokacija);\r\n    } else {\r\n      isError = true;\r\n      // console.log('error with lokacija');\r\n      me.showTimedError(\"Ne postoje podaci za vaš grad u našoj bazi, pokušajte sa nekim drugom gradom.\");\r\n      return false;\r\n    }\r\n\r\n    if (isError) {\r\n      me.showTimedError(\"Došlo je do greške, pokušajte ponovo.\");\r\n      // me.clearForm(form);\r\n    }\r\n\r\n    if (resultList.length === 0) {\r\n      me.showTimedError(\"Ne postoji prašak u našoj bazi koji odgovara datim parametrima. Pokušajte ponovo.\");\r\n      // me.clearForm(form);\r\n    }\r\n\r\n    me.renderProducts(resultList);\r\n\r\n    return false;\r\n  }\r\n\r\n  me.clearForm = (form) => {\r\n    form.find('input').each(\r\n      (index, input) => {\r\n        $(input).val('');\r\n      }\r\n    )\r\n  }\r\n\r\n  me.showTimedError = (errorMessage, time = 5000) => {\r\n    const error = $(me.errorID)[0];\r\n    error.textContent = errorMessage;\r\n\r\n    setTimeout(() => {\r\n      error.textContent = null;\r\n    }, time);\r\n  }\r\n\r\n  $(document).ready(function () {\r\n    me.setListeners();\r\n  });\r\n\r\n})(jQuery.noConflict());"},"lineCount":null}},"hash":"fb1fc217e61312c8ecf350673c43e9b9","cacheData":{"env":{}}}